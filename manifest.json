{
    "manifest_version": 2,
    "name": "Edge Extension V3",
    "version": "1.0",
    "permissions": [
      "webRequest",
      "webRequestBlocking",
      "*://*/*",
      "activeTab",
      "tabs"
    ],
    "browser_action": {
      "default_popup": "popup/popup.html",
      "default_icon": {
        "16": "icons/icon.png",
        "48": "icons/icon.png",
        "128": "icons/icon.png"
      }
    }
  }
  